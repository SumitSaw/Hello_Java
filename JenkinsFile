pipeline{
  agent any 
    stages {
      stage('checkout'){
      steps{
        checkout scm
      }
      }
      
      stage('build'){
      steps{
        sh 'mvn build'
        }
        steps
        {
        sh 'flyway baseline' 
        sh 'flyway migrate'
        sh 'flyway validate' 
        sh 'flyway info'
        }
      }
    }
}
