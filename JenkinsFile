pipeline{
  agent any 
    stages {
      stage('checkout'){
      steps{
        checkout scm
      }
      }
      
      stage('build'){
      steps{
      withMaven(){
        sh 'flyway:baseline' 
        sh 'flyway:migrate'
        sh 'flyway:validate' 
        sh 'flyway:info'
        }
        }
      }
    }
}
