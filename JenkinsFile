pipeline{
  agent any 
    stages {
      stage('checkout'){
      steps{
        checkout scm
      }
      }
      
      stage('build'){
      steps{
      sh 'mvn "flyway:migrate" "flyway:validate" "flyway:info"'  
        }
      }
      post {
		    failure {
				  echo "This will run only if failed";
        
		}
		
		
    }
}
}
