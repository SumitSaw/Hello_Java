  agent any 
    stages {
      stage('checkout'){
      steps{
        checkout scm
      }
      }
      
      stage('build'){
      steps{
      sh 'mvn "flyway:clean" "flyway:baseline" "flyway:migrate" "flyway:validate" "flyway:info"'
        //sh 'flyway:baseline' 
        //sh 'flyway:migrate'
       // sh 'flyway:validate' 
       // sh 'flyway:info'
        
        }
      }
    }
}
